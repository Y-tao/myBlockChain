	<SCRIPT type="text/javascript">
	function submit(){  
		alert('123');
		//document.getElementById("result").setAttribute("style","display");
		var name=document.getElementById('uname').value
		var password=document.getElementById('upass').value
		var data={
			data:JSON.stringify({"name":name,"password":password})
		}
		
		$.ajax({
            url: '/createUser',
            type: 'POST',
			data: data,
            success: function(response){
              //document.getElementById("result").style.display = "";
			  //response=response.data;
			  alert(JSON.stringify(response));
			  //document.getElementById("private_key").innerHTML = response['private_key'];
              //document.getElementById("public_key").innerHTML = response['public_key'];
              //document.getElementById("Error").innerHTML = response['error'];
            }
          });
	}

	</SCRIPT>
	</head>
	<body>
			<div class="row">
				<div class="col-md-4 col-md-offset-4">
					

					<!-- Start Sign In Form -->
					<!--form id = "myform" class="fh5co-form animate-box"
					  action="#" data-animate-effect="fadeIn" onsubmit="return
					  false"-->
					<div class="fh5co-form animate-box">
						<h2>Sign Up</h2>
						<div class="form-group">
							
						<div class="form-group">
							<label for="name" class="sr-only">Name</label>
							<input type="text" class="form-control" name="name" id="uname" placeholder="Name" autocomplete="off" text="">
							<SPAN id="nametext"></SPAN>
						</div>
						<div class="form-group">
							<label for="password" class="sr-only">Password</label>
							<input type="password" class="form-control" \
							name="password" value="" id="upass" placeholder="Password" autocomplete="off">
							<SPAN id="pswtext"></SPAN>
						</div>
						<div class="form-group">
							<label for="re-password" class="sr-only">Re-type Password</label>
							<input type="password"  id="urpass" class="form-control" name="re-password" placeholder="Re-type Password" autocomplete="off">
							<SPAN id="rpswtext"></SPAN>
						</div>
						<div style="display:none" class="form-group" id="result">
							<textarea id="public_key" rows="5" class="form-control" readonly></textarea>
							<textarea id="private_key" rows="5" class="form-control" readonly></textarea>
							<textarea id="Error" rows="5" class="form-control" readonly></textarea>
						</div>
						<!--
						<div class="form-group">
							<label for="email" class="sr-only">Email</label>
							<input type="email" id="uemail" class="form-control" name="email" placeholder="Email" autocomplete="off">
							<SPAN id="emailtext"></SPAN>
						</div>
						<div class="form-group">
							<label for="name" class="sr-only">Country</label>
							<input type="text" class="form-control" name="country" placeholder="Country" autocomplete="off">
						</div>-->
						<!--
						<div class="form-group">
							<p>Already registered? <a href="index.html">Sign In</a></p>
						</div>-->
						<div class="form-group">
            				<input  onclick="javascript:submit();" type="button" class="btn btn-primary" value="Sign up">
							<!--input type="submit" value="Sign Up" class="btn
								btn-primary--">
						</div>
					</div>
					<!-- END Sign In Form -->

				</div>
			</div>
			
		</div>
	
	<!-- jQuery -->
	<script src="{{url_for('static',filename = 'js/jquery.min.js')}}"></script>
